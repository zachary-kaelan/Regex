        <script type="text/javascript">
            $(function() {
                var $table = $(document.getElementById('history-table'));
                var $body = $table.closest('.widget-body');
                var $toggleIcon = $(document.getElementById('alert-history-toggle'));
                var $toggle = $toggleIcon.closest('.widget-header');

                $body.css('display', 'none').removeClass('hide');
                $toggle.css('cursor', 'pointer');
                $toggle.on('click', function() {
                    if ($toggleIcon.hasClass('icon-chevron-down')) {
                        $body.slideDown('fast');
                        $toggleIcon.removeClass('icon-chevron-down').addClass('icon-chevron-up');
                    } else {
                        $body.slideUp('fast');
                        $toggleIcon.removeClass('icon-chevron-up').addClass('icon-chevron-down');
                    }
                });

                Pocomos.table.bind(document.getElementById('history-table'), {
                    sort: [[4, 'desc']]
                });
            });
        </script>
                </div>
                                </div>
    </div>
</div>

