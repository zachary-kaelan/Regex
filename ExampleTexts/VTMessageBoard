    <script type="text/javascript">
        $(function() {
            var form = document.getElementById('office_select'),
                $dropdown = $('.branch-select');
            Pocomos.form.bind(form);
            $(document.getElementById('office_select_office')).change(function() {
                $(form).submit();
            });
            $dropdown.click(function(e) {
                e.stopPropagation();
            });
        });
