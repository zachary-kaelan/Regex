    <script type="text/javascript">
        $(function() {
                        var snd = new Audio("https://myvantagetracker.com/snd/chime.wav?2.17.6-4-1");
            snd.play();
                        var container = document.getElementById('notification-center');
            Pocomos.notifications = new Pocomos.NotificationManager(container, {
                closeRoute: Pocomos.router.closure('notification_complete'),
                close: function(event) {
                    $('.notification-count').text(event.count);
                },
                empty: function() {
                    $(container).find('li.notification').remove();
                }
            });
        });
    </script>
